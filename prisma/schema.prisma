generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model USER {
  id        String      @id @default(uuid())
  name      String
  cpfcpnj   String
  email     String
  password  String
  userType  Int
  createDate DateTime
  sellings  Int
  rate      Float
  imgUrl    String
  contact   String
  contactType Int
}

model HARVEST{
  id        String     @id @default(uuid())
  ownerId   String
  name      String
  description String
  harvestDate DateTime
}

model ADDRESS{
  id        String    @id @default(uuid())
  userId    String
  cep       String
  city      String
}